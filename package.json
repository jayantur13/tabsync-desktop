{
  "name": "tabsync-desktop",
  "version": "1.0.0",
  "description": "Sync your browser tabs across devices with a local server and desktop UI.",
  "main": "main.js",
  "type": "module",
  "author": {
    "name": "Jayant Navrange",
    "email": "vu.vcareforu@gmail.com"
  },
  "productName": "TabSync",
  "license": "MIT",
  "homepage": "https://github.com/jayantur13/tabsync-desktop#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/jayantur13/tabsync-desktop.git"
  },
  "bugs": {
    "url": "https://github.com/jayantur13/tabsync-desktop/issues"
  },
  "keywords": [
    "electron",
    "tabsync",
    "browser-sync",
    "local-server",
    "cross-device",
    "tray-app",
    "utility"
  ],
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "publish": "electron-forge publish",
    "make:linux": "electron-forge make --platform=linux --arch=x64",
    "make:win": "electron-forge make --platform=win32 --arch=x64",
    "make:clean": "rm -rf out/make out/TabSync-*",
    "make:all": "npm run make:clean && npm run make:linux && npm run make:win",
    "release": "node scripts/release.js"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "dotenv": "^17.2.3",
    "electron-squirrel-startup": "^1.0.1",
    "electron-store": "^10.0.0",
    "electron-updater": "^6.3.7"
  },
  "overrides": {
    "@pengx17/electron-forge-maker-appimage": {
      "glob": "10.5.0"
    }
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.8.3",
    "@electron-forge/maker-deb": "^7.10.2",
    "@electron-forge/maker-rpm": "^7.10.2",
    "@electron-forge/maker-squirrel": "^7.10.2",
    "@electron-forge/maker-zip": "^7.10.2",
    "@electron-forge/plugin-auto-unpack-natives": "^7.10.2",
    "@electron-forge/plugin-fuses": "^7.10.2",
    "@electron-forge/publisher-github": "^7.10.2",
    "@electron/fuses": "^1.8.0",
    "@octokit/rest": "^22.0.1",
    "@pengx17/electron-forge-maker-appimage": "^1.2.1",
    "electron": "^39.0.0",
    "prompts": "^2.4.2",
    "simple-git": "^3.30.0"
  },
  "config": {
    "forge": "forge.config.cjs"
  }
}